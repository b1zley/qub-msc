package primitives;

/**
 * 
 */
public class MyOtherPrimitives {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char myChar;

		boolean myBoolean;

		myBoolean = true;

		myChar = 'a';

		

		System.out.println("My Char was "+myChar);

		System.out.printf("My char %c",myChar);
		
//		System.out.println();
		// \n prints new line
		System.out.println("\n\nMy boolean "+myBoolean);
	}

}
