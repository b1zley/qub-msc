public class MyDoubleExample {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		double myDouble;
		myDouble = 3.147;
		
		System.out.println(myDouble);
		System.out.println("My double: " + myDouble);
		
		
		System.out.printf(myDouble);

	}

}
